@model OzdilekBtServisTakipPortalı.ViewModel.KullaniciViewModel
@{
    ViewBag.Title = "ServisTakipKullanici";
    Layout = "~/Views/Shared/_Layout.cshtml";
    
}
<ol class="breadcrumb">
    <li><a href="/Kullanici/ServisTakipKullanici">Servise Gönderilen Ürünler</a></li>
    <li class="active">Servisten Gelen Ürünler</li>
</ol>

<h4><small> Hoş Geldiniz <strong>@ViewBag.kadi</strong></small> </h4>

<h2 class="text-center">Servisten Gelen Ürünler</h2>

<div class="row">
    <div class="col-md-1">
  
  </div>
<div id="dialog-user"  title="Servisten Gelen Ürünler" class="col-md-10">
    @using (Ajax.BeginForm("ServisTakipKullanici1", "Kullanici", FormMethod.Post, new AjaxOptions { OnSuccess = "loadk" }))
    {
        <div class="text-center" >
            Arama: 

            @Html.TextBoxFor(m=>m.ServisGelenarama)
         
            <input class="btn btn-default btn-sm dropdown-toggle" type="submit" value="Ara" name="submit" />
         
        </div>
    }
 </div>
    <div class="col-md-18">
 

    </div>
</div>

<div id="ajax-main">
@Html.Partial("kullaniciservisgelenlist", @Model)
</div>

<br />
<br />
<br />
<br />
<br />
<br />

<script type="text/javascript">

    function loadk(result) {
        $("#ajax-main").html(result);
    }

</script>